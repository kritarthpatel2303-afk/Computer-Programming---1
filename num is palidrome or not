#include<stdio.h>
int main()
{
    int n, rev =0 , rem , temp;
    printf("enter number : ");
    scanf("%d", &n);
    temp = n;
    while (temp > 0) {
        rem = temp%10;
        rev = rev*10 + rem;
        temp/= 10;
    }
    if (rev == n ) printf("%d is palidrome\n", n);
    else printf("%d is not a palidrome\n", n);
    return 0;}
